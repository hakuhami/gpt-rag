<script defer>
        let timezone = Intl.DateTimeFormat().resolvedOptions().timeZone;

        let base = "https://www.alchip.com/en";
        let country = {
            'Asia/Shanghai' : 'https://www.alchip.com/cn',
            'Asia/Tokyo': 'https://www.alchip.com/jp',
            'Asia/Taipei': 'https://www.alchip.com/tw',
        };

let cookie = getCookie('timezone');
if(cookie == null)
{
        if(country[timezone] != undefined && base != country[timezone])
            {
                window.location.href = country[timezone];
            }else if(country[timezone] == undefined&&base != 'https://www.alchip.com/en'){
                window.location.href = 'https://www.alchip.com/en';
            }
        setCookie(1,timezone);
}


function setCookie(day,timezone){
    let toDay = new Date();
    toDay.setDate(toDay.getDate() + day);
    let setDay = toDay.toUTCString();
    document.cookie = "timezone="+timezone+"; expires="+ setDay +"; path=/";
}

function getCookie(name) {
  let cookies = document.cookie.split(';');
  for (let cookie of cookies) {
    let [cookieName, cookieValue] = cookie.split('=');
    if (cookieName.trim() === name) {
      return cookieValue;
    }
  }
  return null;
}
</script>


    
